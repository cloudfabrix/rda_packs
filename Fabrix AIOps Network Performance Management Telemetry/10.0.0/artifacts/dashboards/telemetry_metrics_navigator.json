    "version": "24.1.27.2",
    "short_label": "Stack",
    "live_edit": true,
    "context_label_id0": "",
    "template_variables": {
        "PROJ_ID": {
            "contextId": "projectId"
        },
        "DEVICE_IP": {
            "contextId": "secondaryList_i_agent_host"
        },
        "DEVICE": {
            "contextId": [
                "navigatorSelectionContexts",
                "secondaryListSelection",
                "i_agent_host"
            ]
        },
        "NAME": {
            "contextId": [
                "navigatorSelectionContexts",
                "secondaryListSelection",
                "name"
            ]
        }
    },
    "dashboard_filters": {
        "default_time_filter_labels": [
            "Last 24 hours"
        ],
        "extra_filter0": "projectId in {{PROJ_ID}}",
        "time_filter": true,
        "columns_filter": [
            {
                "id": "i_name",
                "label": "Interfaces",
                "type": "TEXT"
            }
        ],
        "group_filters": [
            {
                "stream": "rdaf_telegraf_metrics",
                "context_filter0": "secondaryList_i_agent_host is i_agent_host",
                "extra_filter": "(name is 'interface') and i_input is 'Telemetry' and i_projectid in {{PROJ_ID}}",
                "title": "Interfaces",
                "group_by": [
                    "i_name"
                ],
                "ts_column": "timestamp",
                "agg": "sum",
                "column": "count_"
            }
        ]
    },
    "navigation_dashboards": {
        "tree_data": {
            "column_name": "i_agent_host",
            "pstream_name": "rdaf_telegraf_metrics",
            "cfxql_query": "i_input is 'Telemetry' and i_projectid in {{PROJ_ID}} and i_agent_host != 'Not Available'",
            "include_icon_url": false,
            "show_counts": false
        },
        "tree_detail": {
            "display_column": "name",
            "selected_columns": {
                "name": "name",
                "i_agent_host": "Device"
            },
            "pstream_name": "rdaf_telegraf_metrics",
            "cfxql_query": "(name in ['cpu', 'interface','memory_usage','device_availability'] and i_projectid in {{PROJ_ID}})",
            "report_definition": {
                "title": "Metrics Checks",
                "widget_type": "tabular",
                "stream": "rdaf_telegraf_metrics",
                "ts_column": "timestamp",
                "timebased": false,
                "columns": {
                    "name": "name",
                    "i_agent_host": "i_agent_host"
                },
                "sorting0": [
                    {
                        "result_int_max": "desc"
                    }
                ],
                "cfxql_query": "i_agent_host is '{{DEVICE}}'",
                "pivot": {
                    "pivot_type": "advanced",
                    "series": [
                        {
                            "column": "name",
                            "agg": "cardinality"
                        }
                    ],
                    "group_by": [
                        "name",
                        "i_agent_host"
                    ]
                },
                "paginated": true,
                "enableVirtualScrolling": false,
                "custom_columns": [
                    {
                        "id": "label1",
                        "expr": "str(name)"
                    }
                ],
                "widget_id": "5afc2391"
            },
            "duration_hours1": 24
        },
        "showNodeIcon": false,
        "showSearchBox": false
    },
