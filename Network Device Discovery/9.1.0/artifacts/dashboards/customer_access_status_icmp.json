{
    "name": "customer_access_status_icmp",
    "label": "Network Collection status",
    "description": "Network collection and access verification status",
    "dashboard_folder": "NetworkDevicePack",
    "version": "1.0.5",
    "enabled": false,
    "live_edit": true,
    "dashboard_type": "template",
    "template_variables": {
        "CUST_ID": {
            "contextId": "customerId"
        },
        "PROJ_ID": {
            "contextId": "projectId"
        },
        "CUST_TAG": {
            "contextId": "customerTag"
        }
    },
    "dashboard_filters": {
        "time_filter": true,
        "default_time_filter_labels": [
            "Last 1 month"
        ],
        "extra_filter": "(customer_tag is '{{CUST_TAG}}' or customer_tag is empty or customer_tag is '' or customer_tag is 'None')",
        "columns_filter": [
            {
                "id": "icmp_status",
                "label": "ICMP Status",
                "type": "TEXT"
            }
        ],
        "group_filters": [
            {
                "stream": "network_access_verification_final",
                "title": "ICMP Status",
                "duration_hours": 1080000,
                "group_by": [
                    "icmp_status"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "_id",
                "limit": 100,
                "type": "int"
            },
            {
                "stream": "network_access_verification_final",
                "title": "SNMP Status",
                "duration_hours": 1080000,
                "group_by": [
                    "snmp_status"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "_id",
                "limit": 100,
                "type": "int"
            },
            {
                "stream": "network_access_verification_final",
                "title": "SSH Status",
                "duration_hours": 1080000,
                "group_by": [
                    "ssh_status"
                ],
                "ts_column": "timestamp",
                "agg": "value_count",
                "column": "_id",
                "limit": 100,
                "type": "int"
            }
        ]
    },
