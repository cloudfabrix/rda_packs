                    "title": "Total CPU's",
                    "widget_type": "custom_counter",
                    "min_width": 3,
                    "max_width": 3,
                    "height": 4,
                    "ts_column": "timestamp",
                    "column": "host_cpus",
                    "paginated": true,
                    "agg": "sum",
                    "type": "str",
                    "include_context_in_query": true,
                    "include_context_keys": [
                        "host_os_ip",
                        "hostname"
                    ],
                    "style": {
                        "color-list": [
                            "#334FFF"
                        ]
                    },
                    "group_by": [
                        "host_os_ip",
                        "hostname"
