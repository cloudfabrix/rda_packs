                    "min_width": 3,
                    "max_width": 3,
                    "height": 4,
                    "ts_column": "timestamp",
                    "column": "host_memory_gb",
                    "paginated": true,
                    "agg": "sum",
                    "type": "str",
                    "include_context_in_query": true,
                    "include_context_keys": [
                        "host_os_ip",
                        "hostname"
                    ],
                    "style": {
                        "color-list": [
                            "#334FFF"
                        ]
                    },
                    "group_by": [
                        "host_os_ip",
                        "hostname"
                    ],
                    "stream": "host-os-inventory",
                    "widget_id": "ec6e986a"
                },
