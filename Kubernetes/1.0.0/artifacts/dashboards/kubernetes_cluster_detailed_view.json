{
    "name": "kubernetes_cluster_detailed_view",
    "description": "Kubernetes Cluster Topology",
    "label": "Kubernetes Clusters Inventory",
    "enabled": false,
    "dashboard_type": "template",
    "context_label_id": "kube_cluster_name",
    "dashboard_style": "tabbed",
    "dashboard_folder": "Kubernetes",
    "live_edit": true,
    "template_variables": {
        "NODE_ID": {
            "contextId": "unique_id"
        },
        "kube_cluster_name": {
            "contextId": "kube_cluster_name"
        },
        "kube_node_ip": {
            "contextId": "kube_node_ip"
        },
        "kube_node_type": {
            "contextId": "kube_node_type"
        },
        "kube_pod_name": {
            "contextId": "kube_pod_name"
        },
        "kube_pod_uuid": {
            "contextId": "kube_pod_uuid"
        },
        "kube_node_hostname": {
            "contextId": "kube_node_hostname"
        },
        "CUST_TAG": {
            "contextId": "customerTag"
        }
    },
    "dashboard_filters": {
        "time_filter": true,
        "columns_filter": [
            {
                "id": "kube_controller_ip",
                "label": "K8s Cluster IP",
                "type": "TEXT"
            },
            {
                "id": "kube_node_hostname",
                "label": "K8s Nodename",
                "type": "TEXT"
            },
            {
                "id": "kube_node_ip",
                "label": "K8s Node IP",
                "type": "TEXT"
            },
            {
                "id": "kube_pod_name",
                "label": "K8s Pod Name",
                "type": "TEXT"
            },
            {
                "id": "kube_pod_container_id_full",
                "label": "K8s Container ID",
                "type": "TEXT"
            },
            {
                "id": "kube_namespace",
                "label": "K8s Namespace",
                "type": "TEXT"
            },
            {
                "id": "kube_service_name",
                "label": "K8s Service Name",
                "type": "TEXT"
            },
            {
                "id": "kube_endpoint_name",
                "label": "K8s Endpoint Name",
                "type": "TEXT"
            },
            {
                "id": "kube_endpoint_ip",
                "label": "K8s Endpoint IP",
                "type": "TEXT"
            }
        ],
        "default_time_filter_labels": [
            "Last 1 month"
        ],
        "group_filters": [
            {
                "stream": "kubernetes-cluster-inventory",
                "extra_filter": "asset_object = 'Node' and (customer_tag is '{{CUST_TAG}}' or customer_tag is empty or customer_tag is 'None')",
                "title": "Clusters",
                "group_by": [
                    "kube_controller_ip"
                ],
                "limit": 200,
                "ts_column": "collection_timestamp",
                "agg": "cardinality",
                "column": "_id",
                "type": "int"
            }
        ]
    },
    "dashboard_sections": [
        {
            "title": "Linux OS Inventory",
            "show_filter": false,
            "widgets": [
                {
                    "widget_type": "label",
                    "max_width": 12,
                    "height": 0.1,
                    "min_width": 12,
                    "label": "<div style='text-align:center;width:100%;'><h1>{{kube_node_hostname}}:{{kube_node_ip}}</h1></div>",
                    "widget_id": "a2e0ffb4"
                },
                {
                    "title": "Server Details",
                    "widget_type": "summary",
                    "stream": "host-os-inventory",
                    "query": "asset_object is 'System' and (customer_tag is '{{CUST_TAG}}' or customer_tag is empty)",
                    "min_width": 6,
                    "height": 4,
                    "max_width": 6,
                    "paginated": true,
                    "max_rows": 100,
                    "include_context_in_query": true,
                    "include_context_keys": [
                        "host_os_ip",
                        "hostname"
                    ],
                    "columns": {
                        "host_os_ip": {
                            "title": "Host OS IP",
                            "key": true
                        },
                        "hostname": "Host Name",
                        "host_machine_vendor": "Host/Hypervisor Vendor",
                        "host_machine_type": "Host Type",
                        "host_os_version": "Host OS Version",
                        "unique_id": {
                            "title": "Unique ID",
                            "key": true,
                            "visible": false
                        },
                        "asset_status": {
                            "title": "Status",
                            "htmlTemplateForRow": "<span class='badge' style='background-color: {{'{{ row.status_colour}}'}}' > {{'{{ row.asset_status}}'}} </span>"
                        },
                        "status_color": {
                            "title": "Color",
                            "type": "COLOR-MAP",
                            "source-column": "host_os_nic_status",
                            "color-map": {
                                "DOWN": "#B71C1C",
                                "UP": "#4CAF50"
                            }
                        },
                        "id": {
                            "title": "ID",
                            "value": "user-dashboard-server_drilldown",
                            "key": true,
                            "type": "FIXED_VALUE",
                            "hidden": true,
                            "visible": false
                        }
                    },
                    "custom_columns": [
                        {
                            "id": "color",
                            "expr": "'#B71C1C' if int(host_os_fs_used_perc) >= 80 else '#4CAF50'",
                            "visible": false
                        },
                        {
                            "id": "status_colour",
                            "expr": "'#4CAF50' if asset_status == 'Active' else '#B71C1C'",
                            "visible": false
                        }
                    ],
                    "widget_id": "1051a855"
                },
                {
                    "title": "Memory (GB)",
