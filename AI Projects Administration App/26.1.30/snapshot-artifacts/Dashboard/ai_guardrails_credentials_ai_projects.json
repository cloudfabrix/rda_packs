{
  "name": "ai_guardrails_credentials_ai_projects",
  "label": "Guardrails Credentials Management Page",
  "description": "Guardrails Credentials Management Page",
  "enabled": true,
  "dashboard_type": "dashboard",
  "dashboard_folder": "AI",
  "dashboard_style": "auto",
  "stream": "rda_secret_meta",
  "dashboard_filters": {
    "time_filter": true,
    "columns_filter": [
      {
        "id": "name",
        "label": "Name",
        "type": "TEXT"
      },
      {
        "id": "type",
        "label": "Type",
        "type": "TEXT"
      },
      {
        "id": "typeDescription",
        "label": "Type Description",
        "type": "TEXT"
      },
      {
        "id": "checksum",
        "label": "Checksum",
        "type": "TEXT"
      },
      {
        "id": "saved_time",
        "label": "Saved Time",
        "type": "DATETIME"
      }
    ]
  },
  "dashboard_sections": [
    {
      "title": "Guardrails",
      "show_filter": true,
      "widgets": [
        {
          "title": "Guardrails",
          "widget_type": "tabular",
          "stream": "rda_secrets_meta",
          "duration_hours": 87600,
          "remote_searchable": true,
          "remote_searchable_cols": [
            "name",
            "type",
            "typeDescription",
            "checksum"
          ],
          "ts_column": "saved_time",
          "paginated": true,
          "extra_filter": "customerTag is empty and type is 'guardrails'",
          "height": 10,
          "min_wdith": 12,
          "columns": {
            "name": {
              "title": "Name",
              "key": true
            },
            "type": {
              "title": "Type",
              "key": true
            },
            "typeDescription": {
              "title": "description"
            },
            "has_credential_check": {
              "title": "Credential Check",
              "visible": false
            },
            "checksum": {
              "title": "Checksum"
            },
            "customerTag": {
              "title": "Customer Tag",
              "type": "TEXT",
              "key": true,
              "hidden": true
            },
            "saved_time": {
              "title": "Saved Time",
              "type": "DATETIME"
            },
            "timestamp": {
              "title": "When",
              "type": "HUMANIZE_TIME_DELTA",
              "source-column": "saved_time",
              "sortable": false,
              "visible": false
            }
          },
          "custom_columns": [
            {
              "id": "rowStyleClass",
              "hidden": true,
              "visiblle": false,
              "expr": "'bg-fading-blue' if time_now_as_ms() - isoformat_to_ms(saved_time) <= 120000 else None"
            },
            {
              "id": "artifact_name",
              "label": "Artifact Name",
              "expr": "name",
              "key": true,
              "visible": false,
              "sortable": false
            },
            {
              "id": "artifact_type",
              "label": "Artifact Type",
              "expr": "'source'",
              "key": true,
              "visible": false,
              "sortable": false
            }
          ],
          "actions": [
            {
              "permission": "agenticai:credential:add",
              "title": "Add",
              "icon": "mdi mdi-plus",
              "type": "POPUP_FORM",
              "selectionType": "NONE",
              "identifier": "saas-service-action:vault-add",
              "api-endpoint": {
                "service-name": "saas-reports",
                "methodName": "getForm",
                "stringified-params": true,
                "parse-output": false,
                "params": [
                  {
                    "formId": "rda.saas.vault.add",
                    "extension_types": [
                      "guardrails"
                    ]
                  }
                ]
              }
            },
            {
              "permission": "agenticai:credential:edit",
              "title": "Edit",
              "type": "POPUP_FORM",
              "selectionType": "SINGLE",
              "identifier": "saas-service-action:vault-edit",
              "api-endpoint": {
                "service-name": "saas-reports",
                "methodName": "getForm",
                "stringified-params": true,
                "parse-output": false,
                "params": [
                  {
                    "formId": "rda.saas.vault.edit"
                  }
                ]
              }
            },
            {
              "permission": "agenticai:credential:view",
              "title": "View",
              "type": "POPUP_FORM",
              "selectionType": "SINGLE",
              "displayLabelColumnId": "name",
              "drillDownLinkField": "name",
              "identifier": "saas-service-action:vault-view",
              "api-endpoint": {
                "service-name": "saas-reports",
                "methodName": "getForm",
                "stringified-params": true,
                "parse-output": false,
                "params": [
                  {
                    "formId": "rda.saas.vault.view"
                  }
                ]
              }
            },
            {
              "permission": "agenticai:credential:verify",
              "title": "Verify Credentials",
              "type": "POPUP_FORM",
              "selectionType": "SINGLE",
              "identifier": "saas-service-action:vault-edit",
              "actionCondition": {
                "actionControl": "HIDE_IF",
                "conditionalField": [
                  {
                    "conditionType": "EQUAL",
                    "conditionValue": "false",
                    "fieldId": "has_credential_check"
                  }
                ]
              },
              "api-endpoint": {
                "service-name": "saas-reports",
                "methodName": "getForm",
                "stringified-params": true,
                "parse-output": false,
                "params": [
                  {
                    "formId": "rda.saas.vault.validateConenctivity"
                  }
                ]
              }
            },
            {
              "permission": "agenticai:credential:assign_tag",
              "title": "Manage Tags",
              "type": "POPUP_FORM",
              "selectionType": "SINGLE",
              "identifier": "saas-service-action:assign-tag",
              "api-endpoint": {
                "methodName": "getForm",
                "params": [
                  {
                    "formId": "rda.saas.tags.assign"
                  }
                ],
                "parse-output": false,
                "service-name": "saas-reports",
                "stringified-params": true
              }
            },
            {
              "permission": "agenticai:credential:delete",
              "title": "Delete",
              "type": "POPUP_FORM",
              "selectionType": "SINGLE",
              "identifier": "saas-service-action:vault-delete",
              "api-endpoint": {
                "service-name": "saas-reports",
                "methodName": "getForm",
                "stringified-params": true,
                "parse-output": false,
                "params": [
                  {
                    "formId": "rda.saas.vault.delete"
                  }
                ]
              }
            },
            {
              "permission": "agenticai:page:view",
              "appName": "user-dashboard/rda-artifact-dependencies-app",
              "identifier": "launch.url.dashboard",
              "selectionType": "SINGLE",
              "stateName": "app.featureapp",
              "title": "View Dependencies",
              "type": "GO_TO_APP_STATE"
            }
          ],
          "widget_id": "9a21b5c6"
        }
      ]
    }
  ]
}