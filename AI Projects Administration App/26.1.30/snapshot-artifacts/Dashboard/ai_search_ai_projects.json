{
  "name": "ai_search_ai_projects",
  "label": "Search AI Artifacts",
  "description": "AI Artifact Search",
  "title": "Search: {{SEARCH_TEXT}}",
  "showTitle": true,
  "is_template": true,
  "enabled": true,
  "dashboard_type": "template",
  "dashboard_folder": "AI",
  "live_edit": true,
  "dashboard_style": "auto",
  "stream": "rda_secret_meta",
  "dashboard_filters": {
    "time_filter": false
  },
  "dashboard_generator": {
    "attachment": "generator"
  },
  "computedContext": {
    "show_personas": {
      "expr": "'yes' if str(show_personas)  in [ 'yes', 'True'] else 'no'",
      "defaultValue": "yes"
    },
    "show_domains": {
      "expr": "'yes' if str(show_domains ) in [ 'yes', 'True'] else 'no'",
      "defaultValue": "yes"
    },
    "show_prompt_templates": {
      "expr": "'yes' if str(show_prompt_templates)  in [ 'yes', 'True'] else 'no'",
      "defaultValue": "no"
    },
    "show_conversations": {
      "expr": "'yes' if str(show_conversations) in [ 'yes', 'True'] else 'no'",
      "defaultValue": "no"
    },
    "show_tool_usage": {
      "expr": "'yes' if str(show_tool_usage) in [ 'yes', 'True'] else 'no'",
      "defaultValue": "no"
    },
    "show_agents": {
      "expr": "'yes' if str(show_agents) in [ 'yes', 'True'] else 'no'",
      "defaultValue": "no"
    }
  },
  "template_variables": {
    "SEARCH_TEXT": {
      "contextId": "search_text",
      "default": ""
    }
  },
  "dashboard_sections": [
    {
      "title": "Personas",
      "show_filter": true,
      "widgets": [
        {
          "context_filter": "show_personas = 'yes'",
          "title": "Personas",
          "widget_type": "tabular",
          "stream": "ai_personas",
          "extra_filter": "(name contains '{{SEARCH_TEXT}}' or description contains '{{SEARCH_TEXT}}'  or llms contains '{{SEARCH_TEXT}}')",
          "duration_hours": 87600,
          "remote_searchable": true,
          "remote_searchable_cols": [
            "name",
            "description",
            "llms"
          ],
          "ts_column": "saved_time",
          "paginated": true,
          "height": 5,
          "min_width": 12,
          "max_width": 12,
          "columns": {
            "name": {
              "title": "Persona Name",
              "key": true
            },
            "ai_project_name": {
              "title": "Project Name",
              "key": true
            },
            "description": {
              "title": "Description",
              "key": false
            },
            "llms": {
              "title": "LLMs",
              "htmlTemplateForRow": "{%raw%}<span class='badge' style='background-color: {{ row.static_color }}' > {{ row.llms }} </span>{%endraw%}"
            },
            "guardrails": {
              "title": "Guardrails",
              "htmlTemplateForRow": "{%raw%}<span class='badge' style='background-color: {{ row.static_color }}' > {{ row.guardrails }} </span>{%endraw%}"
            },
            "color": {
              "title": "Color",
              "key": true,
              "type": "DYNAMIC_HTML",
              "htmlTemplate": "{%raw%}<div class='d-flex'><div style='width:18px;height:18px;margin-right:8px;' :style='{backgroundColor:rowData.color}'></div></div>{%endraw%}"
            }
          },
          "custom_columns": [
            {
              "id": "static_color",
              "expr": "['#3F51B5']",
              "visible": false,
              "hidden": true,
              "label": "Static Color"
            }
          ],
          "actions": [
            {
